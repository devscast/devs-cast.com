{% extends '@layout/backend/index.html.twig' %}

{% block content %}
    <thead>
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Created_at</th>
        <th>online</th>
        <th>archived</th>
        <th>actions</th>
    </tr>
    </thead>
    <tbody>
    {% for item in vm.data %}
        <tr>
            <td>{{ item.id }}</td>
            <td>{{ item.name }}</td>
            <td>{{ item.createdAt | date }}</td>
            <td>{{ active_status(item.isOnline) }}</td>
            <td>{{ active_status(item.isArchived) }}</td>
            <td>
                {{ include('@layout/backend/_actions.html.twig') }}
            </td>
        </tr>
    {% endfor %}
    </tbody>
{% endblock %}
