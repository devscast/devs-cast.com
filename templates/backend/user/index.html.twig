{% extends '@layout/backend/index.html.twig' %}

{% block content %}
    <thead>
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Email</th>
        <th>Last Login At</th>
        <th>Actions</th>
    </tr>
    </thead>
    <tbody>
    {% for item in vm.data %}
        <tr>
            <td>{{ item.id }}</td>
            <td>{{ item.name }}</td>
            <td>{{ item.email }}</td>
            <td>{{ item.lastLoginAt ? item.lastLoginAt| date : '-' }}</td>
            <td>{{ include('@layout/backend/_actions.html.twig') }}</td>
        </tr>
    {% endfor %}
    </tbody>
{% endblock %}
