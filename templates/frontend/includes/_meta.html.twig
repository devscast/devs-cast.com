<meta charset="utf-8">
<meta name="viewport" content="width=device-width initial-scale=1 maximum-scale=1 minimum-scale=1 shrink-to-fit=no">

<meta http-equiv="X-UA-Compatible" content="IE=11.0000">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta http-equiv="content-language" content="{{ meta().lang }}">

<meta name="language" content="{{ meta().lang }}">
<meta name="copyright" content="{{ meta().copyright }}">
<meta name="author" content="{{ meta().author }}">
<meta name="identifier-url" content="{{ meta().url }}">
<meta name="keywords" content="{{ meta().keywords }}" lang="{{ meta().lang }}">

<meta name="robots" content="index,follow"/>
<meta name="classification" content="{{ meta().classification }}">
<meta name="designer" content="{{ meta().url }}">
<meta name="reply-to" content="{{ meta().email }}">
<meta name="owner" content="Devscast ltd.">
<meta name="category" content="Blog">
<meta name="coverage" content="Worldwide">
<meta name="distribution" content="Global">
<meta name="rating" content="General">
<meta name="revisit-after" content="7 days">

<meta name="twitter:card" content="summary"/>
<meta name="twitter:site" content="{{ meta().sitename }}"/>
<meta name="twitter:creator" content="{{ meta().author }}"/>


<meta property="og:language" content="{{ meta().lang }}">
<meta property="og:site_name" content="{{ meta().sitename }}">
<meta property="og:image" content="{{ absolute_url(meta().image) }}">
<meta property="og:image:secure_url" content="{{ absolute_url(meta().image) }}" />
<meta property="og:image:type" content="image/jpeg" />
<meta property="og:image:width" content="400" />
<meta property="og:image:height" content="400" />
<meta property="og:image:alt" content="Devscast - Logo" />
<meta property="og:type" content="website"/>
<meta property="og:url" content="{{ absolute_url(app.request.requestUri) }}">
<meta property="og:email" content="{{ meta().email }}"/>
<meta property="og:phone_number" content="{{ meta().phone }}"/>
<meta property="og:latitude" content="{{ meta().latitude }}"/>
<meta property="og:longitude" content="{{ meta().longitude }}"/>
<meta property="og:street-address" content="{{ meta().street }}"/>
<meta property="og:locality" content="{{ meta().locality }}"/>
<meta property="og:region" content="{{ meta().region }}"/>
<meta property="og:country-name" content="{{ meta().country }}"/>

<meta name="theme-color" content="{{ meta().themeColor }}">
<meta name="msapplication-TileColor" content="{{ meta().themeColor }}">
<meta name="msapplication-navbutton-color" content="{{ meta().themeColor }}">
<meta name="apple-mobile-web-app-status-bar-style" content="{{ meta().themeColor }}">

<meta name="msapplication-TileImage" content="{{ absolute_url('/favicon/ms-icon-144x144.png') }}">
